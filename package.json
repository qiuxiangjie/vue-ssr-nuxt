    "koa": "^2.6.2",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^9.4.0",
    "nuxt": "^2.0.0",
    "prettier": "^2.1.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
